
fun wave_table(x) @[table(size=128,min=0.0,max=1.0)] {
   val w = 6.283185307179586 * x;
   return (sin(w)+sin(3.0*w)+sin(5.0*w)+sin(7.0*w))/4.0;
}

fun osc_wave(cv) {
   return wave_table(Osc_phasor.osc_phasor(cv,false));
}

fun process(cv) {
   return osc_wave(cv);
}

